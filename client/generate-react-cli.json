{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "path": "src/components",
      "customTemplates": {
        "component": "templates/component.tsx"
      },
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "page": {
      "path": "src/pages",
      "customTemplates": {
        "component": "templates/component.tsx"
      },
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "layout": {
      "path": "src/layout",
      "customTemplates": {
        "component": "templates/component.tsx"
      },
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true
    },
    "hook": {
      "customTemplates": {
        "component": "templates/hooks.tsx"
      },
      "path": "src/hooks",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
