{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "path": "src/components",
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx"
      },
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "path": "src/pages",
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx"
      },
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "layout": {
      "path": "src/layout",
      "customTemplates": {
        "component": "templates/layout/TemplateName.tsx"
      },
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "hook": {
      "customTemplates": {
        "component": "templates/hooks/TemplateName.tsx"
      },
      "path": "src/hooks",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
